# Cloud.ru API Configuration
VITE_CLOUDRU_API_KEY=your_cloudru_api_key_here
VITE_CLOUDRU_API_BASE_URL=https://api.cloud.ru

# Application Configuration
VITE_API_BASE_URL=http://localhost:8000/api
VITE_APP_NAME=TestOps Copilot
VITE_APP_VERSION=1.0.0

# GitLab Integration (Optional)
VITE_GITLAB_URL=https://gitlab.example.com
VITE_GITLAB_TOKEN=your_gitlab_token_here
